{
  "$schema": "https://raw.githubusercontent.com/nicerdicer/tauri-apps/dev/crates/tauri-config-schema/schema.json",
  "productName": "Pass GUI",
  "version": "0.1.0",
  "identifier": "com.pass-gui.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Pass - Password Manager",
        "width": 1000,
        "height": 700,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.png"
    ]
  },
  "plugins": {}
}
